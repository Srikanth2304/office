@PostMapping("/updateFormKeyValue")
public ResponseEntity<String> updateFormKeyValue(@RequestBody FormKeyValueUpdateRequest request) {
    try {
        formDataService.updateFormKeyValue(request);
        return ResponseEntity.ok("Form key-values updated/inserted successfully.");
    } catch (Exception ex) {
        return ResponseEntity.status(500).body("Error while updating form key-values: " + ex.getMessage());
    }
}
