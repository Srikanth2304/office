private boolean isMointorLoggingEnabled() {
    LogWriter out = LogWriter.getInstance("RequestAndResponseLoggingFilter", "isMointorLoggingEnabled");
    boolean monitorLog = false;
    out.println("inside isMointorLoggingEnabled >>> ");
    
    // Check if SCBPARAMCache instance is null
    SCBPARAMCache cacheInstance = SCBPARAMCache.getInstance();
    if (cacheInstance == null) {
        out.println("SCBPARAMCache instance is null.");
        return false;
    }

    // Check if cache value is null
    String cacheValue = cacheInstance.getCache("ENABLE_MOINTOR_LOGGING");
    if (cacheValue == null) {
        out.println("ENABLE_MOINTOR_LOGGING cache value is null.");
        return false;
    }

    if (cacheValue.trim().equals("Y")) {
        monitorLog = true;
    }

    out.println("return value >>> " + monitorLog);
    return monitorLog;
}
