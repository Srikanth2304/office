private boolean isGrafanaLoggingEnabled() {
    LogWriter out = LogWriter.getInstance("RequestAndResponseLoggingFilter", "isGrafanaLoggingEnabled");
    boolean grafanaLogFlag = false;
    out.println("inside isGrafanaLoggingEnabled >>> ");

    if ((SCBPARAMCache.getInstance().getCache("ENABLE_GRAFANA_LOGGING") != null)
            && (SCBPARAMCache.getInstance().getCache("ENABLE_GRAFANA_LOGGING").trim().equals("Y"))) {
        grafanaLogFlag = true;
        out.println("return value >>> " + grafanaLogFlag);
    }

    return grafanaLogFlag;
}

